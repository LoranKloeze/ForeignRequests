{
  "name": "Request tracker",
  "version": "0.0.1",
  "description": "Track all request made by a website",
  "devtools_page": "devtools.html",
  "permissions": ["declarativeContent", "activeTab", "storage","webRequest", "webRequestBlocking", "tabs","<all_urls>"  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2

}